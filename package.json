{
  "name": "multi-image-mergetool",
  "description": "Resolve multiple image conflicts at the same time",
  "version": "1.0.0",
  "homepage": "https://github.com/twolfson/multi-image-mergetool",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/multi-image-mergetool.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/multi-image-mergetool/issues"
  },
  "bin": {
    "multi-image-mergetool": "bin/multi-image-mergetool"
  },
  "main": "lib/server",
  "engines": {
    "node": ">= 0.10.0"
  },
  "license": "Unlicense",
  "scripts": {
    "gemini-update": "npm run verify-webdriver-running && gemini update gemini.js",
    "gemini-test": "npm run verify-webdriver-running && gemini test --reporter html --reporter flat gemini.js",
    "precheck": "twolfson-style precheck lib/ test/",
    "lint": "twolfson-style lint lib/ test/",
    "pretest": "twolfson-style install",
    "start": "node bin/multi-image-mergetool",
    "test": "npm run precheck && mocha --reporter dot && npm run lint",
    "verify-webdriver-running": "curl --silent http://localhost:4444/ > /dev/null || (echo \"Webdriver isn't running, please start it via \\`npm run webdriver-start\\`\" 1>&2 && exit 1)",
    "webdriver-start": "xvfb-run webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "async": "~2.1.2",
    "body-parser": "~1.15.2",
    "chalk": "~1.1.3",
    "commander": "~2.9.0",
    "data-uri-to-buffer": "0.0.4",
    "express": "~4.14.0",
    "glob": "~7.1.1",
    "http-errors": "~1.5.0",
    "jade": "~1.11.0",
    "looks-same": "~3.0.0",
    "mkdirp": "~0.5.1",
    "opener": "~1.4.2",
    "tmp": "0.0.29"
  },
  "devDependencies": {
    "gemini": "~4.12.2",
    "foundry": "~4.0.3",
    "foundry-release-git": "~2.0.2",
    "foundry-release-npm": "~2.0.2",
    "jscs": "~1.7.3",
    "jshint": "~2.5.10",
    "mocha": "~1.11.0",
    "twolfson-style": "~1.6.0",
    "webdriver-manager": "~10.2.4"
  },
  "keywords": [
    "image",
    "merge",
    "mergetool",
    "diff",
    "visual",
    "testing"
  ],
  "foundry": {
    "releaseCommands": [
      "foundry-release-git",
      "foundry-release-npm"
    ]
  }
}
